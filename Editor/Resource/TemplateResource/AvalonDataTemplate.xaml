<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:workspace="clr-namespace:Editor.View.Workspace"
                    xmlns:preview="clr-namespace:Editor.View.Preview"
                    xmlns:info="clr-namespace:Editor.View.Info"
                    xmlns:fail="clr-namespace:Editor.View.Fail"
                    xmlns:dock="https://github.com/Dirkster99/AvalonDock"
                    xmlns:inspector="clr-namespace:Editor.View.Inspector"
                    xmlns:vm="clr-namespace:Editor.ViewModel.Preview"
                    >
    <DataTemplate x:Key="SceneNodePreviewViewModelTemplate">
        <preview:SceneNodePreview />
    </DataTemplate>
    <DataTemplate x:Key="_WorkspaceViewModelTemplate">
        <workspace:_Workspace/>
    </DataTemplate>
    <DataTemplate x:Key="ResourceInfoViewModelTemplate">
        <info:ResourceInfo/>
    </DataTemplate>
    <DataTemplate x:Key="FailTemplate">
        <fail:FailView/>
    </DataTemplate>
    <DataTemplate x:Key="ViewportDxViewModelTemplate">
        <workspace:ViewportDx/>
    </DataTemplate>
    <DataTemplate x:Key="InspectorMainViewModelTemplate">
        <inspector:InspectorMain/>
    </DataTemplate>
    <DataTemplate x:Key="ResourcePanelViewModelTemplate">
        <info:ResourcePanel/>
    </DataTemplate>
</ResourceDictionary>